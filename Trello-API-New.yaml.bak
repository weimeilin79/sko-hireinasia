---
swagger: '2.0'
info:
  title: Trello API - New
  description: This document describes the REST API of Trello as published by Trello.com.
  version: 1.0.0
host: customizetwitter-test.192.168.99.100.nip.io
basePath: /twitter
schemes:
  - http
paths:
  /trello:
    post:
      tags:
        - addCards
      summary: addCards()
      description: Add Card
      parameters:
      - name: key
        in: query
        description: API Key
        required: true
        type: string
      - name: token
        in: query
        description: Access Token
        required: true
        type: string
      - name: name
        in: query
        description: The name of the new card.  It is not required if the name           is
          being copied from provided by a URL, file or card that is being copied.
        required: true
        type: string
      - name: desc
        in: query
        description: a string with a length from 0 to 16384
        type: string
      - name: pos
        in: query
        description: A position. top , bottom , or a positive number
        required: false
        type: string
      - name: due
        in: query
        description: A date, or null
        required: true
        type: string
      - name: idList
        in: query
        description: id of the list that the card should be added to
        required: true
        type: string
      - name: labels
        in: query
        description: 'all or a comma-separated list of: blue, green, orange, purple,           red
          or yellow'
        required: false
        type: string
      responses:
        200:
          description: OK String
          schema:
            type: string
